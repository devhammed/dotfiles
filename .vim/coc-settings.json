{
  "coc.preferences.formatOnSaveFiletypes": [
    "dart",
    "ruby",
    "javascript",
    "python",
    "json",
    "vala"
  ],
  "suggest.noselect": false,
  "solargraph.rename": true,
  "solargraph.formatting": true,
  "cSpell.userWords": [
    "solargraph",
    "vala"
  ],
  "python.jediEnabled": false,
  "languageserver": {
    "vala": {
      "command": "vala-language-server",
      "filetypes": [
        "vala"
      ]
    }
  }
}
